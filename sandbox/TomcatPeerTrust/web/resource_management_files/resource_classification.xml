		url="/myapp-0.1-dev/demo/start.html"
		matchingStrategy="_exact_"
		policyName="_no_pol_"
	/>
	
	<resourceClass
		isProtected="false" 
		url="/myapp-0.1-dev/PeerTrustConfig/.*"
		matchingStrategy="_reg_exp_"
